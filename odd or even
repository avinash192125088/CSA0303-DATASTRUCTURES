#include<stdio.h>
int main()
{
	int a,arr[10],even,odd,i;
	printf("enter the size of the array =");
	scanf("%d",&a);
	printf("set of numbers\n");
	for(i=0;i<a;i++)
	{
		scanf("%d",&arr[i]);
	}
	printf("even number=");
	for(i=0;i<a;i++)
	{
		if(arr[i]%2==0)
		{
			printf("%d ",arr[i]);
		}
	}
	printf("\n odd number=");
	for(i=0;i<a;i++)
	{
		if(arr[i]%2!=0)
		{
		printf("%d ",arr[i]);
		}
	}
}
